{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    }
  ],
  "env": {
    "DATABASE_URL": "@process.env.DB_URL"
  },
  "build": {
    "env": {
      "SECRET_KEY": "@process.env.SECRET_KEY"
    }
  }
}
